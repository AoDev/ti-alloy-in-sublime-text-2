					<array>
						<dict>
							<key>include</key>
							<string>#ti-constants</string>
						</dict>
						<dict>
							<key>include</key>
							<string>#numbers</string>
						</dict>
						<dict>
							<key>include</key>
							<string>#hexcolor</string>
						</dict>
						<dict>
							<key>include</key>
							<string>#units</string>
						</dict>
						<dict>
							<key>include</key>
							<string>#property-name</string>
						</dict>
					</array>
				</dict>
			</array>
		</dict>
		<key>rule-id-selector</key>
		<dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>comment</key>
					<string>rule that will match tss clases and id </string>
					<key>patterns</key>
					<array>
						<dict>
							<key>captures</key>
							<dict>
								<key>1</key>
								<dict>
									<key>name</key>
									<string>punctuation.definition.quote.tss</string>
								</dict>
								<key>4</key>
								<dict>
									<key>name</key>
									<string>punctuation.definition.selector.tss</string>
								</dict>
								<key>5</key>
								<dict>
									<key>name</key>
									<string>entity.name.class.selector.tss</string>
								</dict>
								<key>6</key>
								<dict>
									<key>name</key>
									<string>keyword.other.selector.tss</string>
								</dict>
								<key>7</key>
								<dict>
									<key>name</key>
									<string>punctuation.definition.quote.tss</string>
								</dict>
							</dict>
							<key>comment</key>
							<string>group(2) = whole selector without quotes - group(3) class or id type selector</string>
							<key>match</key>
							<string>(")(((#|\.)([_a-zA-Z0-9]+))|([A-Z]+[a-zA-Z]+))(")</string>
							<key>name</key>
							<string>selector.tss</string>
						</dict>
					</array>
				</dict>
			</array>
		</dict>
