							<string>#ti-properties</string>
						</dict>
					</array>
				</dict>
			</array>
		</dict>
		<key>property-value</key>
		<dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>begin</key>
					<string>(?&lt;=[a-z])\s*(:)\s*</string>
					<key>beginCaptures</key>
					<dict>
						<key>1</key>
						<dict>
							<key>name</key>
							<string>punctuation.separator.key-value.tss</string>
						</dict>
					</dict>
					<key>end</key>
					<string>\s*((,)|(?=\})|$)</string>
					<key>endCaptures</key>
					<dict>
						<key>1</key>
						<dict>
							<key>name</key>
							<string>punctuation.terminator.rule.meta.property-list.tss</string>
						</dict>
						<key>2</key>
						<dict>
							<key>name</key>
