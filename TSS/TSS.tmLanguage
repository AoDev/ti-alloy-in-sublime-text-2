	<key>repository</key>
	<dict>
		<key>comments</key>
		<dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>begin</key>
					<string>/\*</string>
					<key>comment</key>
					<string>match block comments</string>
					<key>end</key>
					<string>\*/</string>
					<key>name</key>
					<string>comment.tss</string>
				</dict>
				<dict>
					<key>comment</key>
					<string>match inline comments</string>
					<key>match</key>
					<string>//.*$</string>
					<key>name</key>
					<string>comment.tss</string>
				</dict>
			</array>
		</dict>
		<key>hexcolor</key>
		<dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>match</key>
					<string>#[a-fA-F0-9]+</string>
					<key>name</key>
					<string>entity.name.class.color.tss</string>
				</dict>
			</array>
		</dict>
		<key>numbers</key>
		<dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>match</key>
					<string>[0-9]+</string>
					<key>name</key>
					<string>constant.numeric.tss</string>
				</dict>
			</array>
		</dict>
		<key>property-name</key>
		<dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>begin</key>
					<string>(?&lt;![a-zA-Z])(?=[a-zA-Z])</string>
					<key>end</key>
					<string>(?![a-zA-Z])</string>
					<key>name</key>
					<string>meta.property-name.tss</string>
					<key>patterns</key>
					<array>
						<dict>
